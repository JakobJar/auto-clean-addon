{
  "name": "Auto Clean",
  "description": "Automatically clean your browser data after a certain time.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  },
  "permissions":[
    "storage",
    "browsingData",
    "history"
  ]
}
